<div class="single-container">
  <%= render partial: "/devise/shared/header" %>
  <main class="single-main">
    <section class="signup-container">
      <h2 class="signup-head">新規会員登録</h2>
    <div class="signup-inner">
      <div class="signup-content">
        <div class="btn-default btn-mail">
          <i class="far fa-envelope"></i>
          <%= link_to "メールアドレスで登録", new_user_registration_path %>
        </div>
        <button id="fb-login" class="btn-default btn-sns btn-sns-fb">
          <i class="fab fa-facebook"></i>facebookで登録
        </button>
        <button id="google-login" class="btn-default btn-sns btn-sns-google">
          <i class="fab fa-google-plus-g"></i>Googleで登録
        </button>
      </div>
    </div>
    </section>
  </main>
  <%= render partial: "/devise/shared/footer" %>
</div>
